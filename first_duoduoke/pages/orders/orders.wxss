.bg-white {
    background: #fff;
    color: #000;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.index-top {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
    background: linear-gradient(90deg,#8d68d9,#fc4849,#fb7840);
}

.nav {
    color: #fff;
}

.topNavBar {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    align-content: center;
    height: 40px;
    background: linear-gradient(90deg,#8d68d9,#fc4849,#fb7840);
}

.top_nav,.topNavScroll {
    height: 100%;
}

.top_nav {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    white-space: nowrap;
    font-weight: 550;
}

.nav_opt {
    margin-left: 20rpx;
    font-size: 26rpx;
    padding: 20rpx;
    color: #fff;
}

.nav_opt.active {
    background: #fff;
    border-radius: 20px;
    color: #f43f6c;
    padding: 5rpx 20rpx;
}

.team-top {
    display: flex;
    align-items: center;
    background-color: #fff;
    box-shadow: 0 0 3px #8b8b8b;
    padding: 0 2vw;
    position: relative;
}

.team-title {
    flex: 1;
    height: 35px;
    line-height: 35px;
    font-size: 32rpx;
    text-align: center;
    border-bottom: 2px solid #fff;
    position: relative;
}

.team-c {
    color: #e25371;
}

.weui-navbar__slider {
    position: absolute;
    content: " ";
    left: 0;
    bottom: 0;
    width: 6em;
    height: 2px;
    transition: transform .3s;
    transition: transform .3s,-webkit-transform .3s;
    background-color: #e25371;
}

.seamlessscrolling {
    background-color: #00c201;
    font-size: 24rpx;
    color: #fff;
    padding: 15rpx 25rpx;
    display: flex;
    justify-content: space-between;
}

.seamlessscrolling-more {
    border-radius: 4rpx;
    background-color: #e25371;
    padding: 0 10rpx;
    font-size: 20rpx;
}

.no-order {
    text-align: center;
    padding: 30rpx 0;
    font-size: 24rpx;
    color: #878787;
}

.team-info {
    padding: 0 30rpx;
}

.ord-box-d {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
}

.box-d-ro {
    flex: 1;
    padding: 10rpx;
}

.ord {
    display: flex;
    padding: 20rpx;
    background-color: #fff;
    border: 1px solid #ebeaea;
    margin-top: 15rpx;
    flex-direction: column;
}

.ord-d {
    width: 590rpx;
}

.ord-sn {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0;
}

.ord-sn .ord-sn-l {
    color: #afafaf;
    font-size: 27rpx;
}

.ord-sn .ord-sn-r {
    color: #000;
    font-size: 27rpx;
}

.ord-user {
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.ord-uinfo,.ord-user {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.ord-uinfo {
    justify-content: flex-start;
}

.ord-uinfo image {
    border-radius: 70rpx;
    width: 70rpx;
    height: 70rpx;
}

.ord-uinfo text {
    color: #000;
    font-size: 30rpx;
    margin-left: 10rpx;
}

.ord-stat {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
}

.ord-stat .stat {
    border: 1px solid #afafaf;
    font-size: 26rpx;
    color: #afafaf;
    padding: 0 20rpx;
    border-radius: 10rpx;
    margin-right: 20rpx;
}

.ord-stat .green {
    border: 1px solid #00c201;
    color: #00c201;
}

.ord-stat .red {
    border: 1px solid #fc4849;
    color: #fc4849;
}

.ord-stat .yellow {
    border: 1px solid #ffc107;
    color: #ffc107;
}

.ord-stat .tip {
    border: 1px solid #afafaf;
    font-size: 26rpx;
    color: #afafaf;
    padding: 0 10rpx;
    border-radius: 20rpx;
}

.ord-goods {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    margin-bottom: 30rpx;
}

.ord-img {
    width: 150rpx;
    height: 150rpx;
    margin-right: 20rpx;
}

.ord-l {
    flex: 1;
    flex-flow: column;
    justify-content: space-between;
}

.ord-l,.ord-lt {
    display: flex;
}

.ord-ltl {
    font-size: 30rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    word-break: break-all;
}

.ord-bouns {
    flex-direction: row;
    justify-content: space-around;
    border-top: 1px solid #f1eded;
    padding: 26rpx 0;
}

.ord-bouns,.ord-bouns .ord-price {
    display: flex;
    align-items: center;
}

.ord-bouns .ord-price {
    flex-direction: column;
    width: 50%;
}

.ord-bouns .ord-p-line {
    border-right: 1px solid #f1eded;
}

.ord-price .ord-p-t {
    font-weight: 700;
    color: #000;
    font-size: 36rpx;
}

.ord-price .ord-p-b {
    color: #afafaf;
    font-size: 22rpx;
}

.ord-ltr {
    margin-left: 30rpx;
    font-size: 28rpx;
}

.ord-bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.ord-pa {
    font-size: 26rpx;
    background: #fc4849;
    padding: 0 11rpx;
    color: #fff;
    border-radius: 10rpx;
}

.ord-lb {
    text-align: right;
    font-size: 26rpx;
    color: #afafaf;
}

.red-color {
    color: #f53415;
}

.MGJ-color {
    color: #f07;
}

.WPH-color {
    color: #ff00b1;
}

.SN-color {
    color: #f90;
}

.TB-color {
    color: #ff5000;
}

.icon-tianmao {
    color: #d81e06;
}

.dd-bottom {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 30rpx 0;
    background: linear-gradient(90deg,#f44169,#ef5d4e);
    color: #fff;
    font-size: 36rpx;
    justify-content: center;
    align-items: center;
}

.dd-bottom,.ord-input {
    display: flex;
}

.ord-input {
    justify-content: space-between;
    flex-direction: row;
    padding: 0 30rpx;
    margin-top: 10rpx;
    position: relative;
}

.ord-input input {
    height: 35px;
    line-height: 35px;
    font-size: 26rpx;
    color: #000;
    width: 78%;
    border: 1px solid #ccc;
    padding: 0 10rpx;
}

.ord-input .mini-btn {
    margin: 0;
    font-weight: 700;
}

.ind-input {
    color: #aaa;
}

.ord-input .cancelbtn {
    position: absolute;
    left: 71%;
    z-index: 999;
    top: 14%;
    width: 52rpx;
    height: 52rpx;
    background: rgba(0,0,0,.5);
    border-radius: 32rpx;
}

.KL-color {
    color: #fb2236;
}

.Mt-color {
    color: #ffc107;
}

.SP-color {
    color: #ff6146;
}