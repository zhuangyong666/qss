page {
    background: #23212c;
}

.nav {
    background: transparent;
    z-index: 9999999999;
    position: fixed;
    left: 0;
    right: 0;
}

.nav-color {
    background: #25242c;
    transition: 1s;
}

.nav-title {
    z-index: 9999999999;
}

.back {
    color: #fff;
}

.container-s {
    flex-direction: column;
    padding: 90rpx 20rpx 0;
}

.container-s,.topNav {
    display: flex;
}

.topNav {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    align-content: center;
    width: 100%;
    white-space: nowrap;
    font-size: 32rpx;
    color: #fff;
    position: fixed;
    left: 0;
    right: 0;
    background: #23212c;
    z-index: 99;
}

.nav-item {
    padding: 10rpx 5rpx;
    margin: 0 16rpx;
    display: inline-block;
    text-align: center;
    position: relative;
}

.nav-item>view {
    position: absolute;
    left: 30%;
    right: 30%;
    bottom: 0rpx;
    height: 3px;
    background: #fff;
}

.nav-item-act {
    color: #fff;
    font-size: 36rpx;
    font-weight: 700;
}

.transparent-box {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 3;
    background: transparent;
}

.card-list-box {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.card-list {
    width: 348rpx;
    height: 100%;
    align-items: center;
}

.card,.card-list {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.card {
    width: 100%;
    border-radius: 5px;
    background: #fff;
    margin: 0 0rpx 15rpx;
    overflow: hidden;
    background: #33313c;
}

.card-img {
    width: 100%;
    height: 430rpx;
    position: relative;
}

.img-tag {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    padding: 6rpx 24rpx;
    border-radius: 30px;
    background: rgba(0,0,0,.5);
    flex-direction: row;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
}

.card-bottom,.img-tag {
    display: flex;
}

.card-bottom {
    flex-direction: column;
    width: auto;
    padding: 20rpx;
}

.card-title {
    font-size: 26rpx;
    color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.card-price {
    margin: 10rpx 0;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.now-price {
    font-size: 26rpx;
    color: #fff;
    margin-right: 20rpx;
}

.now-price>text {
    font-size: 28rpx;
}

.old-price {
    font-size: 24rpx;
    color: #898a8a;
    text-decoration: line-through;
}

.card-coupon,.coupon {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.coupon {
    padding: 6rpx 14rpx;
    border-radius: 5px;
    background: linear-gradient(90deg,#d6268a,#5c29dc);
    color: #fff;
}

.coupon,.share {
    font-size: 24rpx;
}

.share {
    color: #ffc460;
    margin-left: 30rpx;
}

.load,.topTag-box {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    align-content: center;
    font-size: 28rpx;
}

.state-1 {
    color: #aaa;
}

.state-1,.state-2 {
    margin-bottom: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.state-2 {
    color: #edd012;
}

.state-3 {
    color: #aaa;
    height: 40rpx;
    transition: height 1s ease-in;
}

.state-3,.state-4 {
    margin-bottom: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.state-4 {
    color: #a5a5a5;
}

.state-5 {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    justify-content: center;
    color: #a5a5a5;
    z-index: 99999999999999;
}

.state-5,.state-7 {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.state-7 {
    margin-bottom: 20rpx;
    color: #848484;
}

.loading {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    margin-right: 10rpx;
}

.loading,.loading__item {
    width: 36rpx;
    height: 36rpx;
}

.loading__item {
    position: absolute;
    top: 0;
    left: 0;
}

.loading__item:before {
    content: "";
    display: block;
    margin: 0 auto;
    width: 4rpx;
    height: 8rpx;
    background-color: #a5a5a5;
    border-radius: 4rpx;
    animation: fadeDelay 1.2s ease-in-out infinite both;
}

.loading__item:nth-child(2) {
    transform: rotate(30deg);
}

.loading__item:nth-child(2):before {
    animation-delay: -1.1s;
}

.loading__item:nth-child(3) {
    transform: rotate(60deg);
}

.loading__item:nth-child(3):before {
    animation-delay: -1s;
}

.loading__item:nth-child(4) {
    transform: rotate(90deg);
}

.loading__item:nth-child(4):before {
    animation-delay: -.9s;
}

.loading__item:nth-child(5) {
    transform: rotate(120deg);
}

.loading__item:nth-child(5):before {
    animation-delay: -.8s;
}

.loading__item:nth-child(6) {
    transform: rotate(150deg);
}

.loading__item:nth-child(6):before {
    animation-delay: -.7s;
}

.loading__item:nth-child(7) {
    transform: rotate(180deg);
}

.loading__item:nth-child(7):before {
    animation-delay: -.6s;
}

.loading__item:nth-child(8) {
    transform: rotate(210deg);
}

.loading__item:nth-child(8):before {
    animation-delay: -.5s;
}

.loading__item:nth-child(9) {
    transform: rotate(240deg);
}

.loading__item:nth-child(9):before {
    animation-delay: -.4s;
}

.loading__item:nth-child(10) {
    transform: rotate(270deg);
}

.loading__item:nth-child(10):before {
    animation-delay: -.3s;
}

.loading__item:nth-child(11) {
    transform: rotate(300deg);
}

.loading__item:nth-child(11):before {
    animation-delay: -.2s;
}

.loading__item:nth-child(12) {
    transform: rotate(330deg);
}

.loading__item:nth-child(12):before {
    animation-delay: -.1s;
}

@keyframes fadeDelay {
    0%,39%,100% {
        opacity: .2;
    }

    40% {
        opacity: 1;
    }
}

@-webkit-keyframes fadeDelay {
    0%,39%,100% {
        opacity: .2;
    }

    40% {
        opacity: 1;
    }
}

.free-loading {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.la-line-scale {
    width: 1.6rem;
    height: 1.28rem;
    display: block;
    font-size: 0;
    color: #fff;
}

.la-line-scale>view {
    display: inline-block;
    float: none;
    background-color: currentColor;
    border: 0 solid;
    width: .16rem;
    height: 1.28rem;
    margin: 0 .08rem;
    border-radius: 0;
    animation: line-scale 1.2s ease infinite;
}

.la-line-scale>view:first-child {
    animation-delay: -1.2s;
}

.la-line-scale>view:nth-child(2) {
    animation-delay: -1.1s;
}

.la-line-scale>view:nth-child(3) {
    animation-delay: -1s;
}

@-webkit-keyframes line-scale {
    0%,40%,to {
        transform: scaleY(.4);
    }

    20% {
        transform: scaleY(1);
    }
}

@keyframes line-scale {
    0%,40%,to {
        transform: scaleY(.4);
    }

    20% {
        transform: scaleY(1);
    }
}